--- linux-2.6.39.2-ck2/include/linux/hookssystem.h	1970-01-01 02:00:00.000000000 +0200
+++ linux-2.6.39.2/include/linux/hookssystem.h	2011-07-23 21:12:18.647582530 +0300
@@ -0,0 +1,42 @@
+#ifndef __HOOKSSYSTEM__H
+#define __HOOKSSYSTEM__H
+
+/* 
+ * HOOKS SYSTEM.
+ * is used to place hooks anywhere in the kernel and to later on implement them somewhere in a module.
+ * for usage example, see kernel/sched_bfs.c, also fs/read_write.c and mod/semstate.c 
+ */
+
+#define NAMENOIMPL(name) name ## _noimpl
+#define NAMEIMPL(name)   name ## _impl
+#define NAMEOLDVAL(name) name ## _oldval
+#define NAMEIDENTY(name) name
+#define NAMEPTR(name,args...) int (*name)(args)
+
+#define COGNITIV_DEFHOOK(name, args...) \
+	static int NAMENOIMPL(name)(args) \
+	{ \
+		return 0; \
+	} \
+	NAMEPTR(NAMEIDENTY(name),args) = NAMENOIMPL(name); \
+	EXPORT_SYMBOL(name)
+
+
+#define COGNITIV_IMPLHOOK(name, args...) \
+	extern NAMEPTR(NAMEIDENTY(name),args); \
+	static NAMEPTR(NAMEOLDVAL(name),args); \
+	static int NAMEIMPL(name)(args)
+
+
+#define COGNITIV_INSTALLHOOK(name) do { \
+	NAMEOLDVAL(name) = NAMEIDENTY(name); \
+	NAMEIDENTY(name) = NAMEIMPL(name); \
+} while(0)
+
+
+#define COGNITIV_UNINSTALLHOOK(name) do { \
+	NAMEIDENTY(name) = NAMEOLDVAL(name); \
+} while (0)
+
+
+#endif
